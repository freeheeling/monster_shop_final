<h2>Add Address</h2>
<%= form_tag '/profile/addresses' do %>
  <%= label_tag :alias %>
  <%= text_field_tag :alias %>
  <%= label_tag :street %>
  <%= text_field_tag :street %>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
  <%= label_tag :state %>
  <%= text_field_tag :state %>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %>
  <%= submit_tag 'Create Address' %>
<% end %>
